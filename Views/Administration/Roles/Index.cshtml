@model IEnumerable<WebRegistry.Models.Roles>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.role)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.description)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.role)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.description)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.iduser_roles">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.iduser_roles">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.iduser_roles">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
